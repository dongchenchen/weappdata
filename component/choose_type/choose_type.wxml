<view class="weui-flex">
	<block wx:for="{{tabs}}" wx:key="{{index}}">
        <view id="{{item.type}}" class="weui-flex__item" bindtap="tabClick">
            <button class="weui-btn mini-btn" type="{{activeIndex == item.type ? 'primary' : 'default'}}" size="mini">{{item.title}}</button>
        </view>
    </block>
</view>