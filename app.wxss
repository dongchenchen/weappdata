/**app.wxss**/

@import "weui.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  margin: 0;
} 

.font-size-24 {
	font-size: 24rpx;
}

mp-header {
	height:120rpx;
	display:block;
	position:fixed;
	top:0;
	left:0;
	right:0;
	z-index:9999;
	background-color: #FFF;
}

mp-choose-type {
	height:70rpx;
	display:block;	
	z-index:0;
	width:100%;
}

.weui-tab__content {
	padding-top:120rpx;
}
.weui-tab__panel {
	padding-top:15rpx;
}

